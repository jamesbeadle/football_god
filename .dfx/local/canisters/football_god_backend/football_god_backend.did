   number: nat8;
   status: nat8;
   totalPot: nat64;
   winningShare: nat64;
 };
type FixtureDTO = 
 record {
   awayTeamGoals: nat8;
   awayTeamId: nat16;
   awayTeamName: text;
   awayTeamPrediction: nat8;
   correct: bool;
   fixtureId: nat32;
   homeTeamGoals: nat8;
   homeTeamId: nat16;
   homeTeamName: text;
   homeTeamPrediction: nat8;
   status: nat8;
 };
type Fixture = 
 record {
   awayGoals: nat8;
