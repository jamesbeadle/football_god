 };
type AdminDTO = 
 record {
   activeGameweekNumber: nat8;
   activeGameweekStatus: text;
   activeSeasonId: nat16;
   activeSeasonName: text;
   seasons: vec SeasonDTO;
 };
type AccountBalanceDTO = record {accountBalance: nat64;};
