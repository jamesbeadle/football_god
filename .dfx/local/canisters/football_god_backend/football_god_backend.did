 };
type GameweekPotDTO = record {gameweekPot: nat64;};
type GameweekDTO = 
 record {
   correctScores: nat8;
   gameweekNumber: nat8;
   sweepstakeEntered: bool;
   totalFixtures: nat8;
   winnings: nat64;
 };
type Gameweek = 
 record {
   fixtureCount: nat8;
